{
  "name": "@xink-sh/xink",
  "version": "0.1.0-next.1",
  "type": "module",
  "keywords": [
    "filesystem",
    "router",
    "bun"
  ],
  "exports": {
    ".": {
      "default": "./index.js",
      "import": "./exports/index.js",
      "types": "./types.d.ts"
    }
  },
  "types": "types.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "bun build --entrypoints ./lib/index.ts ./lib/exports/index.ts --outdir ./dist && tsc && cp ./types.d.ts ./dist/types.d.ts",
    "prep": "cp ./package.json ./dist/package.json && cp ./README.md ./dist/README.md"
  },
  "dependencies": {
    "hexoid": "^1.0.0"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xink-sh/xink.git"
  },
  "bugs": {
    "url": "https://github.com/xink-sh/xink/issues"
  },
  "homepage": "https://github.com/xink-sh/xink#readme"
}
